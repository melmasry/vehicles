@using Vehicles.Web.Models
@model SearchViewModel
@{
    ViewData["Title"] = "Vehicles Status";
}
<div>
    @using (Html.BeginForm("Search", "VehiclesStatus", FormMethod.Post))
    {
        @Html.Partial("SearchPanel", Model);
    }
</div>

<div id="searchResults"></div>

@section scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            InitializeReport("VehiclesStatus","CustomerName");
        });
    </script>

    @* Bootstrap multiselet*@
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="~/js/jquery-ui.js"></script>
    <script src="~/js/bootstrap-multiselect.js"></script>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="~/css/bootstrap-multiselect.css">

    @* Jquery datatable*@
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />

    @* Jquery fixedcolumns*@
    <script src="https://cdn.datatables.net/fixedcolumns/3.2.3/js/dataTables.fixedColumns.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/fixedcolumns/3.2.3/css/fixedColumns.dataTables.min.css" />

    @* Jquery export and print buttons*@
    <script src="https://cdn.datatables.net/buttons/1.4.2/js/dataTables.buttons.min.js"></script>
    <script src="//cdn.datatables.net/buttons/1.4.2/js/buttons.flash.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/pdfmake.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/vfs_fonts.js"></script>
    <script src="//cdn.datatables.net/buttons/1.4.2/js/buttons.html5.min.js"></script>
    <script src="//cdn.datatables.net/buttons/1.4.2/js/buttons.print.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.4.2/css/buttons.dataTables.min.css" />

    @*Datatabl semantic style *@
    <link href="~/css/Semantic-UI-CSS/semantic.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.16/css/dataTables.semanticui.min.css" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/semantic.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/dataTables.semanticui.min.js"></script>

    @* Row group extension*@
    <script src="https://cdn.rawgit.com/ashl1/datatables-rowsgroup/v1.0.1/dataTables.rowsGroup.js"></script>

    @*Report style*@
    <link href="~/css/Report.css" rel="stylesheet" />

    <script src="~/js/SearchManager.js"></script>
}

